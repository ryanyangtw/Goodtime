  <%= search_form_for q ,:url=> search_admin_users_path, :method=> :get,:class=>"navbar-form navbar-left", :role=>"search" do |f| %>
    <div class="form-group">
        <div class="input-group">
          <% value = params[:q][:email_or_name_or_nickname_cont] if params[:q]%>
          <%= f.search_field(:email_or_phone_number_or_name_or_nickname_or_member_id_cont, value: value, :class=>"form-control input-nonhidden", :id=>"navbarInput-01", :type=>"search", :placeholder=>"Search") %>

          
          <span class="input-group-btn">

            <%= button_tag(:type => 'submit', :class => 'btn btn-search') do %>
                <span class="glyphicon glyphicon-search"></span>
            <% end %>

          </span>  
        </div>
    </div>
  <%end%>